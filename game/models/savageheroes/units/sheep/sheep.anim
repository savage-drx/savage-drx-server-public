//syntax:

//makeanim AnimName ClipFile StartFrame NumFrames FPS Loop(0 or 1) Loopbackframe

//common

makeanim idle idle.clip 0 0 30 1 

makeanim melee_1 wound.clip
makeanim melee_move_1 run.clip

makeanim walk_fwd walk.clip 0 0 40 1
makeanim walk_back back.clip 0 0 40 1

makeanim run_fwd run.clip 0 0 50 1
makeanim run_back back.clip 0 0 50 1

makeanim sprint_fwd sprint.clip 0 0 80 1 
makeanim sprint_back sprint.clip 0 0 80 1

makeanim jump_start_fwd jump.clip 0 1
makeanim jump_start_back jump.clip 0 1

makeanim jump_mid_fwd jump.clip 1 1
makeanim jump_mid_back jump.clip 1 1
  
makeanim jump_end_fwd jump.clip 2 1
makeanim jump_end_back jump.clip 2 1
  
makeanim jump_up_end jump.clip 2 1
makeanim jump_land jump.clip 3 1

makeanim wounded_left wound.clip
makeanim wounded_right wound.clip
makeanim wounded_fwd wound.clip
makeanim wounded_back wound.clip

makeanim death_generic death.clip
makeanim resurrected revive.clip 0 30 10

makeanim dodge_start_left jump.clip 0 1
makeanim dodge_start_right jump.clip 0 1
makeanim dodge_start_fwd jump.clip 0 1
makeanim dodge_start_back jump.clip 0 1

makeanim dodge_mid_left jump.clip 1 1
