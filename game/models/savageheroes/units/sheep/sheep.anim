makeanim dodge_mid_fwd jump.clip 1 1
makeanim dodge_mid_back jump.clip 1 1
  
makeanim dodge_end_left jump.clip 2 1
makeanim dodge_end_right jump.clip 2 1
makeanim dodge_end_fwd jump.clip 2 1
makeanim dodge_end_back jump.clip 2 1


