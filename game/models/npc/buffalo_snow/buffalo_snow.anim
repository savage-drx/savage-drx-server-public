// Syntax:

// makeanim AnimName ClipFile StartFrame NumFrames FPS Loop(0 or 1) Loopbackframe

// Common

makeanim idle clip/idle.clip 0 0 30 1

// Melee Combat

makeanim melee_1 clip/charge_fwd.clip 0 0 70 1
makeanim melee_2 clip/charge_fwd.clip 0 0 70 1
makeanim melee_3 clip/charge_fwd.clip 0 0 70 1
makeanim melee_4 clip/charge_fwd.clip 0 0 70 1

makeanim bufcharge1 clip/charge_fwd.clip 0 0 70 1
makeanim bufcharge2 clip/charge_fwd.clip 0 0 70 1
makeanim bufcharge3 clip/charge_fwd.clip 0 0 70 1
makeanim bufcharge4 clip/charge_fwd.clip 0 0 70 1

makeanim melee_charge clip/charge_fwd.clip 0 0 70 1
makeanim melee_release clip/charge_fwd.clip 0 0 70 1
makeanim melee_move_charge clip/charge_fwd.clip 0 0 70 1
makeanim melee_move_release clip/charge_fwd.clip 0 0 70 1
makeanim melee_move_1 clip/charge_fwd.clip 0 0 70 1
makeanim melee_move_2 clip/charge_fwd.clip 0 0 70 1
makeanim melee_move_3 clip/charge_fwd.clip 0 0 70 1
makeanim melee_move_4 clip/charge_fwd.clip 0 0 70 1

// General Character Animation

makeanim walk_left clip/walk_fwd.clip 0 0 45 1
makeanim walk_right clip/walk_fwd.clip 0 0 45 1
makeanim walk_fwd clip/walk_fwd.clip 0 0 45 1
makeanim walk_back clip/walk_fwd.clip 0 0 45 1

makeanim run_left clip/charge_fwd.clip 0 0 70 1
makeanim run_right clip/charge_fwd.clip 0 0 70 1
makeanim run_fwd clip/charge_fwd.clip 0 0 70 1
makeanim run_back clip/charge_fwd.clip 0 0 70 1

makeanim sprint_left clip/charge_fwd.clip 0 0 70 1
makeanim sprint_right clip/charge_fwd.clip 0 0 70 1
makeanim sprint_fwd clip/charge_fwd.clip 0 0 70 1
makeanim sprint_back clip/charge_fwd.clip 0 0 70 1

makeanim wounded_left clip/wounded_left.clip 0 0 30 1
makeanim wounded_right clip/wounded.clip 0 0 30 1
makeanim wounded_fwd clip/wounded.clip 0 0 30 1
makeanim wounded_back clip/wounded.clip 0 0 30 1

makeanim death_generic clip/death_generic.clip 0 0 30 0
makeanim death_left clip/death_generic.clip 0 0 30 0
makeanim death_right clip/death_generic.clip 0 0 30 0
makeanim death_fwd clip/death_generic.clip 0 0 30 0
makeanim death_back clip/death_generic.clip 0 0 30 0

